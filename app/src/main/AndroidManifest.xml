<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dev175.jetpackcomposebasics">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetPackComposeBasics">


        <meta-data
            android:name="android.app.lib_name"
            android:value="" />
        <activity
            android:name=".WidgetsActivity"
            android:exported="true"
            android:label="@string/title_activity_text_style"
            android:theme="@style/Theme.JetPackComposeBasics">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".StateActivity"
            android:exported="true"
            android:label="@string/title_activity_text_style"
            android:theme="@style/Theme.JetPackComposeBasics"/>
        <activity
            android:name=".TextStyleActivity"
            android:exported="true"
            android:label="@string/title_activity_text_style"
            android:theme="@style/Theme.JetPackComposeBasics"/>
        <activity
            android:name=".ModifiersActivity"
            android:exported="true"
            android:label="@string/title_activity_modifiers"
            android:theme="@style/Theme.JetPackComposeBasics" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetPackComposeBasics"/>
    </application>

</manifest>